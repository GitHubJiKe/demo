<script type="text/javascript">
(function(){
$.extend($.fn,{
		mask: function(msg,maskDivClass){
		this.unmask();
		var op = {
		opacity: 0.6,
		z: 8,
		bgcolor: '#000'
		};
		var original=$(document.body);
		var position={top:0,left:0};
		if(this[0] && this[0] !==window.document){
			original=this;
		}
		var maskDiv=$('<span class="maskdivgen"></span>');
		maskDiv.prependTo(original);
		maskWidth=original.find("img").attr("width");
		maskHeight=original.find("img").attr("height");
		if(maskWidth == 104){
			maskWidth = 113;
			maskHeight = 59;
			maskDiv.css({
				position: 'absolute',
				'margin-left':'0px',
				'margin-top':'-3px',
				'z-index': op.z,
				width: maskWidth,
				height:maskHeight,
				'background-color': op.bgcolor,
				zoom:1,
				opacity: 0
				});
		}else{
			maskDiv.css({
			position: 'absolute',
			'margin-left':'0px',
			'margin-top':'0px',
			'z-index': op.z,
			width: maskWidth,
			height:maskHeight,
			'background-color': op.bgcolor,
			zoom:1,
			opacity: 0
			});
		}
	if(maskDivClass){
		maskDiv.addClass(maskDivClass);
	}

	maskDiv.fadeIn('fast', function(){
		$(this).fadeTo('normal', op.opacity);
	})
	return maskDiv;
},
unmask: function(){
	var original=$(document.body);
	if(this[0] && this[0]!==window.document){
		original=$(this[0]);
	}
	original.find("> span.maskdivgen").fadeOut('fast',0,function(){
		$(this).remove();
	});
}
});
})();
	function  addmask(a){
				var aes = $('.'+a+'');
				var count_len = (aes.length)-1;
				for(count_len;count_len>=0;count_len--){
					aes.eq(count_len).mask();
				}
				$('#all_img').hide();
				$('#xz_img').show();
		}
	function movemask(a){
			var aes = $('.'+a+'');
			var count_len = aes.length-1;
			for(count_len;count_len>=0;count_len--){
				aes.eq(count_len).unmask();
			}
			$('#xz_img').hide();
			$('#all_img').show();
		
		}
</script>
	</div>
</div>
