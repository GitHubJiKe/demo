				function global_callback(){
					window.location.href = 'http://my.51job.com/my/My_Pmc.php';
				}
				function loginFromIndex(formobj){
					if(window.cfg.lang == 'c'){
						var language = "CN";
					}else{
						var language = "EN";
					}
					createPassPortLayer(language,global_callback,false);
					$('input[name="passport_loginName"]').attr('value',formobj['username'].value);
					$('input[name="passport_password"]').attr('value',formobj['userpwd'].value);
					passportLogin('CN',function(){global_callback();});
					return false;
				}
			</script>
